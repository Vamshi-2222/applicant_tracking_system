</head>

<body style="margin: 10px;">

    <div class="container-fluid">

        <div class="row">

            <div class="col-md-2"></div>

            <div class="col-md-8">
                <ul id="breadcrumbs-two" style="margin-top: 10px; margin-bottom: 10px;">
                    <li class="active"><a href="#">Personal Information</a></li>
                    <li class="disabled"><a href="#">Educational Information</a></li>
                    <li class="disabled"><a href="#">Experience Information</a></li>
                    <li class="disabled"><a href="#">Submitted Successfully
                        </a></li>
                </ul>
                <!-- Form 1 for Personal Info -->
                <% if(page === 1)  { %>
                    <form action="/candidate/personal-info" method="POST">
                        <div class="card card-primary " id="Form1">
                            <div class="card-header ">
                                <h3 class="card-title ">Personal Information</h3>
                            </div>
                            <!-- /.card-header -->
                            <!-- form start -->
                            <!-- <form role="form "> -->
                            <div class="card-body ">
                                <div class="row ">
                                    <div class="col-md-6 ">
                                        <div class="form-group ">
                                            <label>Source</label>
                                            <select class="form-control select2 " style="width: 100%; " name="sourceData">

                                            <option selected="selected">Select</option>
                                        </select>

                                        </div>
                                    </div>
                                    <div class="col-md-6 ">
                                        <div class="form-group ">
                                            <label for="Reference ">Reference</label>
                                            <input type="text " class="form-control " id="Reference" name="Reference" placeholder="Reference " disabled>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group ">
                                    <label for="name ">Full Name</label>
                                    <input type="text " class="form-control " id="name" name="name" placeholder="Full Name">
                                    <!-- <p style="color: red;">
                                        <%=(typeof errors!='undefined' && typeof errors.name!='undefined' )? errors.name.msg:''%>
                                    </p> -->
                                </div>
                                <label for="name ">Mobile</label>

                                <div class="row " id="files">

                                    <div class="col-md-6 ">
                                        <div class="form-group ">

                                            <input type="number" class="form-control " placeholder="Mobile " name="mobileone" aria-label="Recipient's username" aria-describedby="button-addon2">
                                            <!-- <p style="color: red;">
                                                <%=(typeof errors!='undefined' && typeof errors.mobileone!='undefined' )? errors.mobileone.msg:''%>
                                            </p> -->
                                        </div>
                                    </div>
                                    <div class="col-md-6">

                                        <button type="button" class="btn btn-primary" onclick="addFile();"><i
                                            class="fa fa-plus"></i></button>
                                    </div>
                                </div>

                                <label for="name">Email</label>
                                <div class="row " id="emails">

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <input type="text" class="form-control" placeholder="Email " name="emailone" aria-label="Recipient's username" aria-describedby="button-addon2">
                                            <!-- <p style="color: red;">
                                                <%=(typeof errors!='undefined' && typeof errors.emailone!='undefined' )? errors.emailone.msg:''%>
                                            </p> -->

                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <button type="button" class="btn btn-primary " onclick="addEmail();"><i
                                            class="fa fa-plus"></i> </span></button>
                                    </div>
                                </div>

                                <div class="row ">
                                    <div class="col-md-6 ">
                                        <div class="form-group ">
                                            <label>Hometown</label>
                                            <select class="form-control select2 " style="width: 100%; " name="homeLocation">

                                            <option>Selected</option>
                                        </select>

                                        </div>
                                    </div>
                                    <div class="col-md-6 ">
                                        <div class="form-group ">
                                            <label>Current Location</label>
                                            <select class="form-control select2 " style="width: 100%; " name="currentLocation">
                                            <option selected="selected ">Select</option>

                                    </div>
                                </div>
                            </div>

                            <div class="row ">
                                <div class="col-md-6 ">
                                    <div class="form-group ">
                                        <label>DOB</label>
                                        <input type="date" id="birthday " name="birthday" class="form-control">
                                        <!-- <p style="color: red;">
                                                <%=(typeof errors!='undefined' && typeof errors.birthday!='undefined' )? errors.birthday.msg:''%>
                                            </p> -->
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label>Prefered Location</label>
                                        <div class="s2-example">
                                            <p>
                                                <select class="js-example-basic-multiple" name="preferedLocation"
                                                    multiple="multiple" style="width: 100%;">

                                                </select>


                                            </p>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <!-- /.card-body -->

                        <div class="card-footer ">
                            <input type="submit" class="btn btn-primary" id="first" value="Next">
                            <!-- onclick="executeNext(this.id); return false" -->
                        </div>
                        <!-- </form> -->
            </div>
            </form>
            <% } %>
